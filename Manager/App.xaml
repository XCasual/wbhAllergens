<Application x:Class="AllerConnectManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AllerConnectManager"
             xmlns:converter="clr-namespace:AllerConnectManager.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converter:EnumToDataTemplateConverter x:Key="Enum2DataTemplateConverter"/>
        <converter:SentinelConverter x:Key="SentinelConverter"/>
        <converter:BinaryToImageConverter x:Key="BinaryToImageConverter"/>
    </Application.Resources>
</Application>
